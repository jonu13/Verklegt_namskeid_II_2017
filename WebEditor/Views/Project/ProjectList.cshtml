@model WebEditor.Models.ViewModels.ProjectViewModel
@{
    ViewBag.Title = "ProjectList";
}
<div class="navbar navbar-inverse navbar-fixed-top">
	<div class="container">
		<div class="navbar-header">
			<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
			</button>
			@Html.ActionLink("WebEditor", "ProjectList", "Project", new { area = "" }, new { @class = "navbar-brand" })
		</div>
		<div class="navbar-collapse collapse">
			<ul class="nav navbar-nav">
				<li>@Html.ActionLink("Projects", "ProjectList", "Project")</li>
			</ul>
			@Html.Partial("_LoginPartial")
		</div>
	</div>
</div>

<h2>@Model.project.name</h2>

@if (Model.users.Count == 0)
{
	<p>No user</p>
}
else
{
	<ul>
		@foreach (var user in Model.users)
		{
			<li>@Html.ActionLink(@user.userName, "EditFile", "Project", new { id = user.id }, null)</li>
		}
	</ul>
}