@model List<WebEditor.Models.Entities.Project>
@{
    ViewBag.Title = "ProjectList";
}
<div class="navbar navbar-inverse navbar-fixed-top">
	<div class="container">
		<div class="navbar-header">
			<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
			</button>
			@Html.ActionLink("WebEditor", "ProjectList", "Project", new { area = "" }, new { @class = "navbar-brand" })
		</div>
		<div class="navbar-collapse collapse">
			<ul class="nav navbar-nav">
				<li>@Html.ActionLink("Projects", "ProjectList", "Project")</li>
			</ul>
			@Html.Partial("_LoginPartial")
		</div>
	</div>
</div>

<h2>Your Projects</h2>
<ul>
        @foreach (var project in Model)
        {
            <li>
                @project.name
                <ul>
                    @foreach (var file in project.files)
                    {
                        <li>@Html.ActionLink(@file.fileName, "EditFile", "Project", new { id = file.fileID }, null)</li>
                    }
                </ul>
            </li>
        }
</ul>